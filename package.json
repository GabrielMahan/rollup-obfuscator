{
  "name": "rollup-obfuscator",
  "version": "3.0.2",
  "description": "A plugin to obfuscate javascript for rollup & vite based on https://www.npmjs.com/javascript-obfuscator",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:tsup": "tsup-node --watch",
    "dev:rollup": "rollup -w -c dev/rollup.config.js",
    "build": "tsup-node"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^20.5.3",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.28.1",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  },
  "license": "MIT",
  "keywords": [
    "obfuscator",
    "rollup",
    "rollup obfuscator",
    "rollup plugin",
    "plugin",
    "rollup plugin obfuscator",
    "rollup-obfuscator",
    "javascript obfuscator",
    "vite obfuscator",
    "vite",
    "vite-obfuscator"
  ],
  "homepage": "https://github.com/ghostdevv/rollup-obfuscator",
  "repository": {
    "type": "git",
    "url": "https://github.com/ghostdevv/rollup-obfuscator"
  },
  "bugs": {
    "url": "https://github.com/ghostdevv/rollup-obfuscator"
  },
  "dependencies": {
    "@rollup/pluginutils": "^5.0.3",
    "javascript-obfuscator": "^4.0.2"
  }
}